<h2>Edit Band</h2>
<%= form_for @band, :url => { :action => "update" }, :html => {:class => "form"} do |f| %>
<div class="field">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div class="field">
  <%= f.label :image%>
  <%= f.text_field :image%>
</div>
<%= f.submit "Edit Band" %>
<% end %>