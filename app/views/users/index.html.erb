<h3>Users</h3>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %> signed up <%= user.created_at.to_date %>
      <% if current_user.admin %>
		<%= link_to "delete", user, method: :delete,
                                  data: { confirm: "You sure?" } %>
	  <% end %>
    </li>
  <% end %>
</ul>
